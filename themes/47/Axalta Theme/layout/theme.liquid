      {% include 'multi_storefront_passcode' %}

      <!-- HEADER -->
      <div id="header">
        <div id="wrapper">
          <div class="top-area clrfix">
            <!-- LOGO -->
            <div class="left">
              {% if settings.show_logo %}
              <a href="/">
                <div class="logo"></div>
              </a>
              {% endif %}
            </div>


            <div class="right clrfix">

              <!-- NAVIGATION BAR -->
              <div class="navigation-bar">
                  <div id="dropdown">
                    <i id="open" class="fa fa-bars visible"></i>
                    <i id="close" class="fa fa-times hidden"></i>
                  </div>

              <div id="menu">

                <div class="navigation-bar">
                    <div id="top-menu">
						
         {% category_navigation full:true %} 
						<script>$('#top-menu ul.level-0 ');</script>
						{% if current_user and wishlist_enabled %}
                  <script>$('#top-menu ul.level-0 ').append("<li><a href='{{ wishlist_url }}' class='cart'><i class='fa fa-heart'></i> ( {{ wishlist.line_items.size }} items )</a></li>");</script>
                {% endif %}

                {% if shop.store_enabled? %}
                  <script>$('#top-menu ul.level-0 ').append("<li><a href='{{ cart_url }}' class='cart'><i class='fa fa-shopping-cart'></i> {% if settings.show_shopping_cart_total %} {{ order.item_with_setup_charge_total | money_with_currency }} {% endif %} ({{ order.line_items.size }} items )</a></li>");</script>
                {% endif %}
						 {% if authentication_enabled %}
                  <div class="account-links">
                    {% if current_user %}
                    <a href="{{ account_dashboard_url }}">{{ current_user.first_name }} {{ current_user.last_name }}</a>
                    <a href="{{ logout_url }}">Log Out</a>
                    {% else %}
                      <script>$('#top-menu ul.level-0 ').append("<li><a href='{{ login_url }}'>Login</a></li>");</script>
                     <script>$('#top-menu ul.level-0 ').append("<li><a href='{{ signup_url }}'>Register</a></li>");</script>
                    {% endif %}
                    <!-- {% if shop.catalog_enabled? and settings.search_on %}
                    <a id="searchdropdown"><i class="fa fa-search"></i></a>
                    {% endif %} -->
                  </div>
                {% endif %}
                      <!--{% main_navigation %}-->
                    </div>
                    <div class="clear"></div>
                

                <!-- CART LINKS -->
                <!--{% if shop.store_enabled? and current_user %}
                  <a href="{{ account_balance_url }}" class="balance-info">{% if shop.points_store? %}Points{% else %}Account{% endif %} Balance: <span>{{ current_user.balance | points_or_currency }}</span></a>
                {% endif %}--> 
<div class="cart">
                

                <!-- ACCOUNT LINKS -->
               

                <!-- SEARCH FEATURE -->
                {% if shop.catalog_enabled? and settings.search_on %}
                  <div id="search" class="search-wrapper">
                    {% form 'search', context_name: 'f', url: '/products' %}
                    <div class="search clrfix">
                      {% form_field 'f.text_field', 'keyword', { "class": "input-text clearHint", "placeholder": "Search", "value": "" }  %}
                      {% form_field 'f.submit', '', { "class": "button" } %}
                    </div>
                    {% endform %}
                  </div>
                {% endif %}
              </div>
            </div>
          </div>
        </div>
			  </div> 
      </div>
			</div>
      <!-- END HEADER -->

    </div>

    <!-- NOTIFICATIONS -->
    <div class="notification-area">
      {% for notice in flash_notices %}
      <div class="notification notification-success">{{ notice }}</div>
      {% endfor %}
      {% for error_message in flash_errors %}
      <div class="notification notification-error">{{ error_message }}</div>
      {% endfor %}
    </div>


    <!-- SLIDER -->
    {% if template_name == 'index' %}
    <div id="slideshow">
      {% if settings.show_carousel %}
          {% include 'carousel' %}
      {% endif %}
    </div>
    {% endif %}

    <!-- MAIN CONTENT -->
    <div class="content clrfix">
      <div id="wrapper" class="main_content clrfix {% if page.is_home_page %}home{% endif %}">
        {{ content_for_layout }}
      </div>
    </div>
  </div>

  <!-- FOOTER -->
  <div id="footer">
    <div class="footer-content clrfix">
      <!-- TOP FOOTER BOXES -->
      <div class="footer-top clrfix">
        <div class="col col_4">
          <h1>{{ settings.footer_box_1_heading }}</h1>
          {{ settings.footer_box_1_content }}
        </div>

        <div class="col col_4">
          <h1>{{ settings.footer_box_2_heading }}</h1>
          {{ settings.footer_box_2_content }}
        </div>

        <div class="col col_4">
          <h1>{{ settings.footer_box_3_heading }}</h1>
          {{ settings.footer_box_3_content }}
        </div>
      </div>


      <!-- FOOTER BOTTOM -->
      <div class="footer-bottom clrfix">
        <div class="col col_3" style="display:none">
          <h1>Navigation</h1>
          <span class="line"></span>
            {% main_navigation %}
        </div>

        <div class="col col_3">
          <h1>{{ settings.footer_bbox2_heading }}</h1>
          <span class="line"></span>
          <p>{{settings.footer_bbox2_content}}</p>
        </div>

        <div class="col col_3">
          <h1>{{ settings.footer_bbox3_heading }}</h1>
          <span class="line"></span>
          {{ settings.footer_bbox3_content }}
        </div>

        <div class="col col_3">
          <h1>{{ settings.footer_bbox4_heading }}</h1>
          <span class="line"></span>
          {{ settings.footer_bbox4_content }}
        </div>
      </div>

      <!-- FOOTER COPYRIGHT -->
      <div class="footer-copy">
          <p>{{ settings.copyright_info }}</p>
      </div>

    </div>
  </div>

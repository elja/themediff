html { overflow-y: auto; }
body {
  overflow-x:hidden;

  font-family: {{ settings.sitewide_font_family }};
  font-size:{{ settings.sitewide_font_size }};
  color:{%if settings.content_text_color.size != 0%}{{ settings.content_text_color }}{%else%}inherit{%endif%};

  background: url({{ settings.sitewide_bg_image | asset_url }}) {{settings.sitewide_bg_repeat}} {{settings.sitewide_bg_position}};
  background-color: {%if settings.sitewide_bg_color.size != 0%}{{ settings.sitewide_bg_color }}{%else%}inherit{%endif%};


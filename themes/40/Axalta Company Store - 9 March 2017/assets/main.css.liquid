/*** Global ***/
html {
  height:100%;
}
#main-container {
  min-height:100%;
  position:relative;
}

/**
* @section Main Layouts
* @subsection Top Bar
*/
.top-bar{
  background: {{ settings.topbar_background_color }};
}

.top-bar a, .top-bar a:link, .top-bar a:visited{
  display: inline-block;
  color: {{ settings.topbar_link_color }};
  /*text-decoration: none;*/
  font-size: {{ settings.topbar_font_size }};
  /*font-size: .9em;*/
  padding: 10px;
  -webkit-transition: all .25s linear;
  -moz-transition: all .25s linear;
  -ms-transition: all .25s linear;
  -o-transition: all .25s linear;
  transition: all .25s linear;
}

.top-bar a:hover, .top-bar a:active {
  color: {{ settings.topbar_link_color_hover }};
}

.top-bar .social:hover{background-color:{{ settings.topbar_social_color_hover}};}

/**
* @section Main Layouts
* @subsection Header
*/
#header {
  font-size: {{ settings.header_font_size }};
  background: {{ settings.header_bg_color}};
  behavior: url({{ "PIE.htc" | asset_url }});
  position:relative;
  padding: 18px 0px;
}
.lt-ie10 #header { position: relative; }
.lt-ie9 #header { position: relative; }
#content.two-col { min-height: 300px!important;}
#wrapper {
  padding-bottom: 70px;
  width: {{settings.site_width}};
  margin: 0 auto;
  position: relative;
}
.top-bar #wrapper, .navigation-bar #wrapper, #header #wrapper, .category-navigation #wrapper{padding-bottom: 0px;}


/**
* @section Main Layouts
* @subsection Footer
*/
#footer {
  background-color: {{ settings.footer_bg_color }};
  position:relative;
  margin: -70px auto 0px;
  z-index: 1;
  text-transform: uppercase;
}
#footer a:link, #footer a:visited {
  /*color: #d0d0d0;*/
  text-decoration: none;
  /*font-size: .9em;*/
  -webkit-transition: all .25s linear;
  -moz-transition: all .25s linear;
  -ms-transition: all .25s linear;
  -o-transition: all .25s linear;
  transition: all .25s linear;
}
#footer a:hover, #footer a:active {
  /*padding-left: 5px;*/
  /*color: #f5f5f5;*/
}

#footer h1 {
  color: {{ settings.footer_heading_color }};
  font-size: {{ settings.footer_heading_font_size }};
  text-transform: uppercase;
  margin: 0 10px 10px 10px;
  font-weight: 900;
  display: inline-block;
  letter-spacing: 1px;
}

#footer h1 + .line {
  margin: 5px 10px 10px 10px;
}

#footer .line {
  display: block;
  height: 2px;
  width: 20%;
  z-index: -10;
}

#footer p {
  margin: 0 10px 0 10px;
  font-size: {{ settings.footer_font_size }};
  line-height: 1.25em;
}

#footer ul {
  list-style: none;
  float:none;
  text-align: left;
}

#footer ul li {
  /*margin-left: 30px;*/
  padding: 5px 10px;
  list-style: none;
  display: list-item;
}

.footer-top{
  {% if settings.show_footer_blocks == false %}
  display: none;
  {% endif %}
}

.footer-top h1 {
  color: {{ settings.footer_box_heading_color }}!important;
  text-transform: uppercase;
  margin: 0 10px 10px 10px;
  /*font-weight: 400;*/
  font-size: {{ settings.footer_box_heading_font_size }};
  display: inline-block;
  padding: 30px 10px 0px 10px;
  display: block;
}

.footer-top a:link, .footer-top a:visited {
  display: block;
  position: relative;
  color: {{ settings.footer_box_link_color }}!important;
  font-size: {{ settings.footer_box_link_font_size }};
  padding: 0px 20px 30px 20px;
  background-color:transparent;
}
.footer-top a:hover, .footer-top a:active {padding-left: 20px!important; color: {{ settings.footer_box_link_color_hover}}!important; background-color:transparent;}


.footer-top .col{margin-bottom: 40px;}
.footer-top .col:nth-child(1){background: {{ settings.footer_box_1_color }};}
.footer-top .col:nth-child(2){background: {{ settings.footer_box_2_color }};}
.footer-top .col:nth-child(3){background: {{ settings.footer_box_3_color }};}


.footer-bottom{padding-bottom: 40px;}
.footer-bottom i, .footer-bottom ul ul, .footer-bottom em{display: none!important;}

.footer-copy{padding: 20px 0;}
#footer em, #footer i {height:100%; display: inline-block;}
#main-container > em, #main-container > i{position: absolute;width:19px;top: 0;display: block;}


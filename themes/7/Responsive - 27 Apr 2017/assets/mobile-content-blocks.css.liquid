/* HOME: PRODUCTS */
/*-----------------------------------*/
 #homeProducts .product{
        float: left;
        width: calc(33.3% - 140px);
        padding: 10px 70px;
        text-align: center;
    }

    #homeProducts .product img{
        width:100%!important;
        height:auto!important;
        margin-bottom: 10px;
        border-bottom: 3px solid;
    }

    #homeProducts .product h2{
        font-size: 1em;
        font-style: italic;
    }

    #homeProducts hr{
        opacity: 0.25;
        width: 45%;
    }

    @media (max-width: 890px){
        #homeProducts .product{
            width: calc(33.3% - 60px);
            padding: 10px 30px;
        }
    }

    @media (max-width:481px) {
        #homeProducts .product{
            float: none;
            width: calc(100% - 60px);
        }
    }


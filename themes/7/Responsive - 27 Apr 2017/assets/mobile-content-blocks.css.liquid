/*-----------------------------------*/
/* CUSTOM HOME */
/*-----------------------------------*/
#home .row{
	margin: 40px 0px;
    display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
}

#home .img_row{
    display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	text-align: center;
}


#home .col{
	position: relative;
	-webkit-flex:1;
    -moz-flex:1;
    -ms-flex:1;
	flex: 1;
}

#home .border{
  border: 1px solid #c8c8c8;
}

#home .green_dark{
  border-top: 5px solid rgb(105,190,40);
}

#home .green_medium{
  border-top: 5px solid #2bad2d;
}

#home .green_light{
  border-top: 5px solid rgb(190, 214, 0);
}

#home .wrapper{padding: 20px;}

#home h2{
    font-size: 1.75em;
    line-height: 1.3em;
    margin: 0px 0px 15px;
    font-family: "Vitesse", serif!important;
	font-weight: bold;
    color: #000;
    position: relative;
    z-index: 1;
}

#home h3{
    font-size: 1.3em;
    line-height: 1.3em;
    margin: 0px 0px 15px;
    font-family: "Vitesse Bold", serif;
    color: #000;
    position: relative;
    z-index: 1;
}

#home h4{
	font-size: 0.8em;
    font-family: Helvetica, Arial, sans-serif;
    font-weight: 100;
    color: #5e5e5e;
    position: relative;
    z-index: 1;
}

#home p, #home ul li{
	font-size: 1em;
    line-height: 1.5em;
    font-family: Helvetica, Arial, sans-serif;
    font-weight: 100;
    color: #5e5e5e;
    position: relative;
    z-index: 1;
}

#home a{text-decoration: none; color:#5e5e5e;font-weight: 100;}

#home ul{margin: 0px;}

#home ul li{
	list-style-image: url("{{ 'img_bullet.png' | asset_url}}");
    margin: 10px 0px;
    list-style-position: inside;
}

.gray_button{
    width: 160px;
    height: 40px;
    background: #aaaaaa;
    color: #FFF!important;
    font-family: "Helvetica CY", "Helvetica Neue Condensed", "Arial Narrow", sans-serif;
    font-weight: bold;
    text-transform: uppercase;
    text-align: center;
    line-height: 44px;
    position: absolute;
    bottom: 25px;
}

.green_button{
    width: 160px;
    height: 40px;
    background: #34b233;
    font-family: "Helvetica CY", "Helvetica Neue Condensed", "Arial Narrow", sans-serif;
    font-weight: bold;
    text-transform: uppercase;
    text-align: center;
    line-height: 44px;
    position: absolute;
    bottom: 25px;
}

.green_button a, .green_button a:link, .green_button a:visited{
	color: #FFF!important;
}

.box_1_img{
	position: absolute;
    bottom: 0px;
    right: 20px;
    height: 100%;
    max-height: 244px;
}

@media (max-width: 660px){
	#home .row{
		display:block;
	}

	#home .col{margin: 10px!important; text-align: center;}

	.gray_button, .green_button{
		position: relative;
		margin: 0 auto;
		bottom: auto;
	}

	.box_1_img{display: none;}
}
